{
  "name": "spinkit",
  "version": "1.2.5",
  "description": "A collection of loading indicators animated with CSS",
  "keywords": [
    "css",
    "spinkit",
    "spinner",
    "loading",
    "ui"
  ],
  "filename": "spinkit.min.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobiasahlin/SpinKit.git"
  },
  "author": "Tobias Ahlin",
  "license": "MIT",
  "homepage": "https://github.com/tobiasahlin/SpinKit",
  "npmName": "spinkit",
  "npmFileMap": [
    {
      "basePath": "css",
      "files": [
        "spinkit*.css",
        "spinners/1-rotating-plane*.css",
        "spinners/10-fading-circle*.css",
        "spinners/11-folding-cube*.css",
        "spinners/2-double-bounce*.css",
        "spinners/3-wave*.css",
        "spinners/4-wandering-cubes*.css",
        "spinners/5-pulse*.css",
        "spinners/6-chasing-dots*.css",
        "spinners/7-three-bounce*.css",
        "spinners/8-circle*.css",
        "spinners/9-cube-grid*.css"
      ]
    }
  ],
  "requiredFiles": [
    "spinners/1-rotating-plane.min.css",
    "spinners/10-fading-circle.min.css",
    "spinners/11-folding-cube.min.css",
    "spinners/2-double-bounce.min.css",
    "spinners/3-wave.min.css",
    "spinners/4-wandering-cubes.min.css",
    "spinners/5-pulse.min.css",
    "spinners/6-chasing-dots.min.css",
    "spinners/7-three-bounce.min.css",
    "spinners/8-circle.min.css",
    "spinners/9-cube-grid.min.css"
  ]
}
